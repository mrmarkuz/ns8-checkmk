{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "ignorePaths": ["**/ui/**", "**/.devcontainer/**"],
  "regexManagers": [
    {
      "fileMatch": ["build-images.sh"],
      "matchStrings": ["docker\\.io/checkmk/check-mk-raw:(?<currentValue>[-0-9\\.a-z]+)"],
      "datasourceTemplate": "docker",
      "depNameTemplate": "checkmk/check-mk-raw"
    }
  ]
}
